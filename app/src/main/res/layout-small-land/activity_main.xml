<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frontpagelayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity"
    android:background="@drawable/lg">


    <Button
        android:id="@+id/bsq"
        android:layout_width="70dp"
        android:layout_height="20dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="79dp"
        android:layout_marginTop="43dp"
        android:background="@drawable/button_custom"
        android:text="@string/sq"
        android:textColor="#ffffff"
        android:textSize="12sp" />

    <Button
        android:id="@+id/btq"
        android:layout_width="70dp"
        android:layout_height="20dp"
        android:layout_alignParentTop="true"
        android:layout_marginStart="126dp"
        android:layout_marginTop="43dp"
        android:layout_toEndOf="@+id/bsq"
        android:background="@drawable/button_custom"
        android:text="@string/tq"
        android:textColor="#ffffff"
        android:textSize="12sp" />

    <Button
        android:id="@+id/bseeotherapps"
        android:layout_width="70dp"
        android:layout_height="20dp"
        android:layout_below="@id/btq"
        android:layout_alignParentStart="true"
        android:layout_marginStart="76dp"
        android:layout_marginTop="112dp"
        android:background="@drawable/button_custom"
        android:text="@string/seeother"
        android:textColor="#ffffff"
        android:textSize="12sp" />

    <Button
        android:id="@+id/brateapp"
        android:layout_width="70dp"
        android:layout_height="20dp"
        android:layout_below="@+id/btq"
        android:layout_marginStart="127dp"
        android:layout_marginTop="112dp"
        android:layout_toEndOf="@+id/bseeotherapps"
        android:background="@drawable/button_custom"
        android:text="@string/rateapp"
        android:textColor="#ffffff"
        android:textSize="12sp" />
</RelativeLayout>