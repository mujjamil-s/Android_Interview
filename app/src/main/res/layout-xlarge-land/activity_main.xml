<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frontpagelayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity"
    android:background="@drawable/lg">


    <Button
        android:id="@+id/btq"
        android:layout_width="350dp"
        android:layout_height="70dp"
        android:layout_alignParentTop="true"
        android:layout_marginStart="90dp"
        android:layout_marginTop="114dp"
        android:layout_toEndOf="@+id/bsq"
        android:background="@drawable/button_custom"
        android:text="@string/tq"
        android:textColor="#ffffff"
        android:textSize="30sp" />

    <Button
        android:id="@+id/brateapp"
        android:layout_width="350dp"
        android:layout_height="70dp"
        android:layout_below="@+id/btq"
        android:layout_marginStart="83dp"
        android:layout_marginTop="201dp"
        android:layout_toEndOf="@+id/bseeotherapps"
        android:background="@drawable/button_custom"
        android:text="@string/rateapp"
        android:textColor="#ffffff"
        android:textSize="30sp" />

    <Button
        android:id="@+id/bseeotherapps"
        android:layout_width="350dp"
        android:layout_height="70dp"
        android:layout_below="@id/btq"
        android:layout_alignParentStart="true"
        android:layout_marginStart="144dp"
        android:layout_marginTop="202dp"
        android:background="@drawable/button_custom"
        android:text="@string/seeother"
        android:textColor="#ffffff"
        android:textSize="30sp" />

    <Button
        android:id="@+id/bsq"
        android:layout_width="350dp"
        android:layout_height="70dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="135dp"
        android:layout_marginTop="113dp"
        android:background="@drawable/button_custom"
        android:text="@string/sq"
        android:textColor="#ffffff"
        android:textSize="30sp" />

</RelativeLayout>