<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frontpagelayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity"
    android:background="@drawable/lg">


    <Button
        android:id="@+id/btq"
        android:layout_width="300dp"
        android:layout_height="60dp"
        android:layout_alignParentTop="true"
        android:layout_marginStart="90dp"
        android:layout_marginTop="88dp"
        android:layout_toEndOf="@+id/bsq"
        android:background="@drawable/button_custom"
        android:text="@string/tq"
        android:textColor="#ffffff"
        android:textSize="15sp" />

    <Button
        android:id="@+id/brateapp"
        android:layout_width="300dp"
        android:layout_height="60dp"
        android:layout_below="@+id/btq"
        android:layout_marginStart="102dp"
        android:layout_marginTop="127dp"
        android:layout_toEndOf="@+id/bseeotherapps"
        android:background="@drawable/button_custom"
        android:text="@string/rateapp"
        android:textColor="#ffffff"
        android:textSize="15sp" />

    <Button
        android:id="@+id/bsq"
        android:layout_width="300dp"
        android:layout_height="60dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="53dp"
        android:layout_marginTop="85dp"
        android:background="@drawable/button_custom"
        android:text="@string/sq"
        android:textColor="#ffffff"
        android:textSize="15sp" />

    <Button
        android:id="@+id/bseeotherapps"
        android:layout_width="300dp"
        android:layout_height="60dp"
        android:layout_below="@id/btq"
        android:layout_alignParentStart="true"
        android:layout_marginStart="49dp"
        android:layout_marginTop="131dp"
        android:background="@drawable/button_custom"
        android:text="@string/seeother"
        android:textColor="#ffffff"
        android:textSize="15sp" />

</RelativeLayout>